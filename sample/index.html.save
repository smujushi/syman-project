	<script>
	$(function(){
		$("path").hover(
			function(){
				$(this).css("fill-opacity", "1");
			},
			function(){
				$(this).css("fill-opacity", "0.25");
			}
		)
		.mouseDown(function(e){
			$(this).css("fill-opacity", "1");)
			switch($(this).attr("id")){
				case "redQuad":
					// A Note
					break;
				case "greenQuad":
					// E Note, Octave lower
					break;
				case "blueQuad":
					// E Note
					break;
				case "yellowQuad":
					// C# Note
					break;
			}
		})
		.mouseUp(function(e){
			$(this).css("fill-opacity", "0.25");
		});
	});
	</script>
